@model Dictionary<string, string>

<form method="post" asp-action="Add" asp-controller="HashType" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name" />
    <input type="text" name="value" />
    <button type="submit">Kaydet</button>
</form>

@foreach (var item in Model)
{
    <h2>@item.Key -> @item.Value <a asp-controller="HashType" asp-action="DeleteItem" asp-route-name="@item.Key">Sil</a></h2>
}